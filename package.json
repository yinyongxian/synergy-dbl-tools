{
  "name": "synergy-dbl-tools",
  "displayName": "Synergy DBL Tools",
  "description": "Synergy DBL Tools",
  "version": "0.0.16",
  "publisher": "synergy-dbl-tools",
  "license":"MIT",
  "icon": "Media/icon-tool.ico",
  "repository": {
    "type": "git",
    "url": "https://github.com/yinyongxian/synergy-dbl-tools"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "copy-file-name",
        "title": "Copy Filename"
      },
      {
        "command": "copy-file-name-without-extension",
        "title": "Copy Filename Without Extension"
      },
      {
        "command": "copy-break-command",
        "title": "Copy Break Command"
      },
      {
        "command": "copy-debug-compilation-command",
        "title": "Copy Debug Compilation Command"
      },
      {
        "command": "copy-release-compilation-command",
        "title": "Copy Release Compilation Command"
      },
      {
        "command": "copy-debug-compilation-command-and-show-result",
        "title": "Copy Debug Compilation Command And Show Result"
      },
      {
        "command": "copy-release-compilation-command-and-show-result",
        "title": "Copy Release Compilation Command And Show Result"
      },
      {
        "command": "copy-examine-command",
        "title": "Copy Examine Command"
      },
      {
        "command": "copy-examine-and-watch-command",
        "title": "Copy Examine And Watch Command"
      },
      {
        "command": "copy-watch-command",
        "title": "Copy Watch Command"
      },
      {
        "command": "copy-deposit-command",
        "title": "Copy Deposit Command"
      },
      {
        "command": "copy-line-number",
        "title": "Copy Line Number"
      },
      {
        "command": "copy-go-line-number",
        "title": "Copy Go Line Number"
      },
      {
        "command": "copy-go-return",
        "title": "Copy Go Return"
      },
      {
        "command": "copy-go-exit",
        "title": "Copy Go Exit"
      },
      {
        "command": "copy-cancel-all",
        "title": "Copy Cancel All"
      },
      {
        "command": "copy-trace",
        "title": "Copy Trace"
      },
      {
        "command": "copy-return-exit-cancel-trace",
        "title": "Copy Return/Exit/Cancel/Trace"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "copy-break-command",
          "group": "9_cutcopypaste@11"
        },
        {
          "command": "copy-debug-compilation-command",
          "group": "9_cutcopypaste@12"
        },
        {
          "command": "copy-release-compilation-command",
          "group": "9_cutcopypaste@13"
        },
        {
          "command": "copy-debug-compilation-command-and-show-result",
          "group": "9_cutcopypaste@14"
        },
        {
          "command": "copy-release-compilation-command-and-show-result",
          "group": "9_cutcopypaste@15"
        },
        {
          "command": "copy-examine-command",
          "group": "9_cutcopypaste@16"
        },
        {
          "command": "copy-watch-command",
          "group": "9_cutcopypaste@17"
        },
        {
          "command": "copy-examine-and-watch-command",
          "group": "9_cutcopypaste@18"
        },
        {
          "command": "copy-deposit-command",
          "group": "9_cutcopypaste@19"
        },
        {
          "command": "copy-line-number",
          "group": "9_cutcopypaste@20"
        },
        {
          "command": "copy-go-line-number",
          "group": "9_cutcopypaste@21"
        },
        {
          "command": "copy-go-return",
          "group": "9_cutcopypaste@22"
        },
        {
          "command": "copy-go-exit",
          "group": "9_cutcopypaste@23"
        },
        {
          "command": "copy-cancel-all",
          "group": "9_cutcopypaste@24"
        },
        {
          "command": "copy-trace",
          "group": "9_cutcopypaste@25"
        },
        {
          "command": "copy-return-exit-cancel-trace",
          "group": "9_cutcopypaste@26"
        }
      ],
      "editor/title/context": [
        {
          "command": "copy-file-name",
          "group": "2_copy"
        },
        {
          "command": "copy-file-name-without-extension",
          "group": "2_copy"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}