{
  "name": "synergy-dbl-tools",
  "displayName": "Synergy DBL Tools",
  "description": "Synergy DBL Tools",
  "version": "0.0.1",
  "publisher": "YongXian Yin",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "synergy-dbl-tools.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "synergy-dbl-tools.copy-file-name",
        "title": "Copy File Name"
      },
      {
        "command": "synergy-dbl-tools.copy-file-name-without-extension",
        "title": "Copy File Name Without Extension"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "synergy-dbl-tools.copy-file-name",
          "group": "9_cutcopypaste"
        },
        {
          "command": "synergy-dbl-tools.copy-file-name-without-extension",
          "group": "9_cutcopypaste"
        }
      ],
      "editor/title/context": [
        {
          "command": "synergy-dbl-tools.copy-file-name",
          "group": "2_copy"
        },
        {
          "command": "synergy-dbl-tools.copy-file-name-without-extension",
          "group": "2_copy"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}